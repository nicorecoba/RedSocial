@{
}
@using Obligatorio2


@using Biblioteca
@model List<Publicacion>


@foreach(Publicacion p in Model)
{

    <h3>@p.Titulo</h3>
    <p>@p.Texto</p>
}
@if (TempData["error"] != null)
{
    <p>@TempData["Mensaje"]</p>
}